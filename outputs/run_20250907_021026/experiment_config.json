{
    "model": "Enhanced Attention U-Net",
    "task": "segmentation",
    "input_shape": [
        256,
        256,
        3
    ],
    "num_classes": 1,
    "batch_size": 8,
    "epochs": 50,
    "learning_rate": 0.001,
    "optimizer": "Adam",
    "loss": "BinaryCrossentropy",
    "use_self_attention": false,
    "use_channel_attention": false,
    "use_deep_supervision": false,
    "attention_type": "spatial",
    "metrics": [
        "dice_coefficient",
        "iou_coefficient"
    ]
}